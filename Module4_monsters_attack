import java.util.*;
import java.lang.*;

class Monster{
 String name;
 Monster (String name){
 this.name=name;
 }
 void attack()
 {
   System.out.println("attacked");     
  }

}

class Firemonster extends Monster{
 Firemonster(String name)
{
        super(name);
}

void attack()
{
        System.out.println(name+" is attacked by Firemonster");
}

}

class Watermonster extends Monster{
 Watermonster(String name)
{       
        super(name);
       }
       
       void attack()
       {
        System.out.println(name+" is attacked ny Watermonster");
       }
}

class Stonemonster extends Monster{
Stonemonster(String name)
{
        super(name);
}
void attack()
{
        System.out.println(name+" is attackd by Stonemonster");
}

}

class test{
public static void main(String a[])
{
        Monster sc=new Firemonster("Salar");
        sc.attack();
        Monster ak=new Watermonster("BILLA");
        ak.attack();
        Monster sk=new Stonemonster("Rebal");
        sk.attack();

}

}
